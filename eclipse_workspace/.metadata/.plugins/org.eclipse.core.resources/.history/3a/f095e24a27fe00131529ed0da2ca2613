package com.hexlet;
import Course;
import Student;

import java.io.IOException;

public class Main {

	public static void main(String[] args) throws IOException {
		Student studentMax = new Student();
		studentMax.course = new Course();
		studentMax.course.name = "Java course";
		studentMax.name = "Max";
		studentMax.sayHello();
		//System.out.println(studentMax.course);
		Student studentVb = new Student();
		studentVb.course = new Course();
		studentVb.sayHello();
	}

}
