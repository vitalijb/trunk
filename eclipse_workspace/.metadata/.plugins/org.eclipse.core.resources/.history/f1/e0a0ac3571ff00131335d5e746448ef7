package com.hexlet.lesson4;



public class Human {
	private String name;
	
	private int age;
	
	public void setAge(int newAge) {
		if (newAge > 0 && newAge < 120)  {
			age = newAge;
		}
	}
	
	public int getAge() { 
		return age;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String newName) {
		name = newName;
	}
}
