package com.hexlet.lesson4;

public class Human {
	public String name;
	
	public int age;
	
	public void setAge(int newAge) {
		if (newAge > 0) {
		age = newAge;
		} else {
			System.out.println("age setting error");
		}
	}
	
	public int getAge() { 
		return age;
	}
	
	public String getName() {
		return name;
	}

	public void setName(String newName) {
		name = newName;
	}
}
